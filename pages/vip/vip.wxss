.title{
  height: 400rpx;
  width: 750rpx;
  background-color: rgb(5, 46, 44);
  
  position: relative;
  z-index: 9;
}
.title-text{
  width: 702rpx;
  height: 100rpx;
  position: absolute;
  left: 24rpx;
  bottom: 30rpx;

  background: linear-gradient(175deg, rgb(6, 91, 87), rgb(14, 115, 17));
  color: rgb(244, 227, 175);
  border-radius: 30rpx 30rpx 0 0;
  text-align: center;
  line-height: 100rpx;
  font-weight: bold;
}
.title-text text{
  font-size: 40rpx;
}
.con {
  width: 750rpx;
  height: 700rpx;
  background-color: #fff;
  position: relative;
  top: -30rpx;
  z-index: 999;
  border-radius: 30rpx;
}
.vip-title{
  text-align: center;
  height: 100rpx;
  font-weight: bold;
  line-height: 100rpx;
  color: rgb(85, 51, 29);
}
.vip-title text{
  font-size: 40rpx;
  color: rgb(17, 118, 65);
}
.RAndI{
  width: 702rpx;
  height: 200rpx;
  margin: 0 0 0 24rpx;
  background-color: rgb(255, 242, 204);
  border-radius: 20rpx;
}